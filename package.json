{
    "name":  "quiet-strength-blog",
    "version":  "0.1.0",
    "private":  true,
    "dependencies":  {
                         "lodash.deburr":  "^4.1.0",
                         "@testing-library/jest-dom":  "^5.17.0",
                         "@testing-library/react":  "^13.4.0",
                         "@testing-library/user-event":  "^13.5.0",
                         "framer-motion":  "^12.23.5",
                         "fs-extra":  "^11.3.1",
                         "globby":  "^14.1.0",
                         "jsdom":  "^26.1.0",
                         "puppeteer":  "^24.16.0",
                         "react":  "^18.2.0",
                         "react-dom":  "^18.2.0",
                         "react-helmet-async":  "^2.0.5",
                         "react-router-dom":  "^6.30.1",
                         "react-scripts":  "5.0.1",
                         "web-vitals":  "^2.1.4"
                     },
    "scripts":  {
                    "clean":  "rimraf node_modules .next dist build .turbo .cache",
                    "predev":  "node ./scripts/predev.js",
                    "dev":  "npm run predev \u0026\u0026 react-scripts start",
                    "dev:express":  "node dev-server.js",
                    "start":  "react-scripts start",
                    "build":  "react-scripts build",
                    "build:basic":  "react-scripts build",
                    "postbuild":  "cross-env WRITE=1 node scripts/optimize-images.mjs \u0026\u0026 node scripts/apply-critters.mjs \u0026\u0026 cross-env HOST=https://trueallyguide.com node scripts/apply-canonicals-all.js",
                    "preview":  "npx serve -s build",
                    "test":  "react-scripts test",
                    "eject":  "react-scripts eject",
                    "analyze":  "npm run build \u0026\u0026 npx webpack-bundle-analyzer build/static/js/*.js",
                    "lighthouse":  "lhci autorun",
                    "seo-check":  "echo \u0027Running SEO analysis...\u0027 \u0026\u0026 npm run build",
                    "generate-sitemap":  "node scripts/generate-sitemap.js",
                    "generate-sitemap-dynamic":  "node scripts/generate-sitemap-dynamic.js",
                    "sitemap-watch":  "node scripts/sitemap-watcher.js",
                    "sitemap-status":  "node scripts/sitemap-watcher.js --status",
                    "test:visual":  "playwright test tests/visual.spec.js",
                    "test:performance":  "playwright test tests/performance.spec.js",
                    "test:e2e":  "playwright test",
                    "test:url-canonicals":  "playwright test tests/canonical-urls.spec.js",
                    "test:install":  "playwright install",
                    "redirects:test":  "node scripts/test-redirects.js",
                    "assets:test":  "node scripts/test-assets.js",
                    "ci:validate-assets":  "node scripts/ci-asset-validation.js",
                    "prerender":  "node scripts/prerender.mjs",
                    "test:prerender":  "playwright test tests/prerender.spec.js",
                    "validate:canonicals":  "node scripts/validate-canonical-urls.js",
                    "validate:canonicals:dev":  "node validate-canonicals-dev.js",
                    "validate:seo:enhanced":  "node validate-enhanced-seo.js",
                    "seo:validate":  "npm run build \u0026\u0026 npm run validate:seo:enhanced",
                    "seo:validate:dev":  "npm run validate:canonicals:dev",
                    "mini": "npm run build && node scripts/quick-validate-seo.mjs",
                    "fast:playwright": "playwright test tests/prerender.spec.js --project=chromium --workers=1 --reporter=line",
                    "fast:seo": "node scripts/quick-validate-seo.mjs",
                    "fast": "npm run build \u0026\u0026 npm run test -- --watchAll=false \u0026\u0026 npm run fast:playwright \u0026\u0026 npm run fast:seo",
                    "full": "npm run build \u0026\u0026 npm run test \u0026\u0026 npm run test:prerender \u0026\u0026 npm run validate:seo:enhanced",
                    "test:canonical-raw-html":  "playwright test tests/canonical-raw-html.spec.js",
                    "optimize-images":  "node scripts/optimize-images.mjs",
                    "deploy:vercel":  "npx vercel --prod --confirm --name quiet-strength-blog-plbo",
                    "deploy:vercel:token":  "npx vercel --prod --confirm --name quiet-strength-blog-plbo --token %VERCEL_TOKEN%"
                },
    "eslintConfig":  {
                         "extends":  [
                                         "react-app",
                                         "react-app/jest"
                                     ]
                     },
    "browserslist":  {
                         "production":  [
                                            "\u003e0.2%",
                                            "not dead",
                                            "not op_mini all"
                                        ],
                         "development":  [
                                             "last 1 chrome version",
                                             "last 1 firefox version",
                                             "last 1 safari version"
                                         ]
                     },
    "devDependencies":  {
                            "cross-env":  "^7.0.3",
                            "rimraf":  "^5.0.5",
                            "@playwright/test":  "^1.54.2",
                            "@tailwindcss/typography":  "^0.5.16",
                            "cheerio":  "^1.1.2",
                            "chokidar":  "^4.0.3",
                            "critters":  "^0.0.23",
                            "express":  "^5.1.0",
                            "http-proxy-middleware":  "^3.0.5",
                            "playwright":  "^1.54.2",
                            "sharp":  "^0.34.3",
                            "tailwindcss":  "^3.3.3",
                            "webpack-bundle-analyzer":  "^4.10.2",
                            "xml2js":  "^0.6.2",
                            "@lhci/cli":  "^0.13.0"
                        }
}
